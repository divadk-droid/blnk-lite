{
  "version": 2,
  "name": "blnk-burn-tracker",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@blnk_api_url",
    "NEXT_PUBLIC_WS_URL": "@blnk_ws_url",
    "NEXT_PUBLIC_CHAIN_ID": "8453"
  }
}
